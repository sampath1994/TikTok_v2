<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="70" prefWidth="142" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="org.openjfx.FXMLController">
    <children>
        <Button fx:id="set" layoutX="3.0" layoutY="40.0" mnemonicParsing="false" onAction="#set" prefHeight="28.0" prefWidth="22.0" text="S" />
        <Button fx:id="pause" layoutX="32.0" layoutY="40.0" mnemonicParsing="false" onAction="#pause" prefHeight="28.0" prefWidth="22.0" text="P" />
        <Button fx:id="reset" layoutX="60.0" layoutY="40.0" mnemonicParsing="false" onAction="#reset" prefHeight="28.0" prefWidth="22.0" text="D" />
        <Button fx:id="close" layoutX="115.0" layoutY="40.0" mnemonicParsing="false" onAction="#close" prefHeight="28.0" prefWidth="22.0" text="X" />
        <Button fx:id="minimize" layoutX="88.0" layoutY="40.0" mnemonicParsing="false" onAction="#minimize" prefHeight="28.0" prefWidth="22.0" text="_" />
        <Label fx:id="time" layoutX="57.0" layoutY="14.0" text="Label" />
        <Label fx:id="distraction" layoutX="107.0" layoutY="14.0" text="Dis" />
    </children>
</AnchorPane>
